{
  "name": "ai-automation-workflow",
  "version": "0.1.0",
  "private": true,
  "description": "A minimal MCP-enabled automation workflow engine (n8n-like).",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "example:basic": "tsx examples/workflow-basic.ts",
    "example:mcp": "tsx examples/workflow-mcp.ts",
    "example:json": "tsx examples/run-json.ts",
    "server:dev": "tsx server/index.ts",
    "web:dev": "vite --config web/vite.config.ts",
    "dev:all": "concurrently \"npm:server:dev\" \"npm:web:dev\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dagre": "^0.8.5",
    "express": "^4.21.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.10.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^9.0.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
